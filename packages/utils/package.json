{
  "name": "@thread/utils",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    "./date": {
      "import": {
        "types": "./dist/date.d.ts",
        "default": "./dist/date.js"
      },
      "require": {
        "types": "./dist/date.d.cts",
        "default": "./dist/date.cjs"
      }
    },
    "./assertion": {
      "import": {
        "types": "./dist/assertion.d.ts",
        "default": "./dist/assertion.js"
      },
      "require": {
        "types": "./dist/assertion.d.cts",
        "default": "./dist/assertion.cjs"
      }
    },
    "./request": {
      "import": {
        "types": "./dist/request.d.ts",
        "default": "./dist/request.js"
      },
      "require": {
        "types": "./dist/request.d.cts",
        "default": "./dist/request.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "build:pkg": "tsup",
    "clean": "rm -rf .turbo node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "prettier": "@thread/prettier-config",
  "dependencies": {
    "date-fns": "catalog:"
  },
  "devDependencies": {
    "@thread/eslint-config": "workspace:*",
    "@thread/prettier-config": "workspace:*",
    "@thread/tsconfig": "workspace:*",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:"
  }
}
